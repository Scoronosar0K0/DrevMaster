{
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.17.0",
      "PYTHON_VERSION": "3.9"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install --force",
  "framework": "nextjs"
}
